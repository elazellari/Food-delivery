<div class="navigation-bar">
  <a href="/" class="logo"><img src="/images/logo.png" alt="Bite Rush logo" /></a>
  <a href="/">Home</a>
  <a href="/mealkits">On the Menu</a>
  <% if (typeof user === "undefined") { %> 
      <a href="/sign-up">Sign Up</a>
      <a href="/log-in">Log In</a>
  <% } else { %> 
      <% if (userLogin === "clerk") { %> 
          <a href="/mealkits/list"><%= user.firstName %></a>
      <% } else { %> 
          <a href="/cart"><%= user.firstName %></a>
      <% } %>
      <a href="/logout">Logout</a>
  <% } %>
</div>

